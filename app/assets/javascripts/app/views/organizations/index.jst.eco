<h1>Listing Organizations</h1>

<% for organization in @organizations: %>
  <div class="item" data-id="<%= organization.id %>">
    <a data-type="show"><%= organization.name %></a>
    
    <a data-type="edit">Edit</a> 
    <a data-type="destroy">Destroy</a>
  </div>
<% end %>

<p><a data-type="new">New Organization</a></p>